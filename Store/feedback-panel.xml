<?xml version="1.0" standalone="no"?>
<!DOCTYPE swatml SYSTEM "http://swat.silverorange.com/swatml1.dtd">
<swatml>
	<widget class="SwatForm" id="feedback_form">
		<property name="action">feedback</property>
		<property name="classes[]">clearfix</property>
		<widget class="SwatFrame" id="feedback_content">
			<widget class="SwatContentBlock" id="feedback_description">
				<property name="content_type">text/xml</property>
			</widget>
			<widget class="SwatFormField" id="bodytext_field">
				<widget class="SwatTextarea" id="bodytext">
					<property name="required" type="boolean">true</property>
					<property name="rows" type="integer">6</property>
					<property name="cols" type="integer">50</property>
				</widget>
			</widget>
			<widget class="SwatFormField" id="email_field">
				<property name="title" translatable="yes">Email</property>
				<property name="note">Email is optional, but if youâ€™d like a reponse, please do include it.</property>
				<widget class="SwatEmailEntry" id="email">
					<property name="maxlength" type="integer">255</property>
				</widget>
			</widget>
			<widget class="SwatFooterFormField" id="submit_button_field">
				<widget class="SwatButton" id="submit">
					<property name="title" translatable="yes">Send Feedback</property>
				</widget>
			</widget>
		</widget>
	</widget>
</swatml>
