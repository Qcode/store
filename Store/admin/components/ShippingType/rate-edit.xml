<?xml version="1.0" standalone="no"?>
<!DOCTYPE swatml SYSTEM "http://swat.silverorange.com/swatml1.dtd">
<swatml>
	<widget class="SwatMessageDisplay" id="message_display" />
	<widget class="SwatFrame" id="edit_frame">
		<property name="title" translatable="yes">Shipping Rate</property>
		<widget class="SwatForm" id="edit_form">
			<widget class="SwatFormField">
				<property name="title" translatable="yes">Region</property>
				<widget class="SwatFlydown" id="region">
					<property name="required" type="boolean">true</property>
				</widget>
			</widget>
			<widget class="SwatFormField">
				<property name="title" translatable="yes">Threshold</property>
				<widget class="SwatMoneyEntry" id="threshold">
					<property name="required" type="boolean">true</property>
				</widget>
			</widget>
			<widget class="SwatFormField">
				<property name="title" translatable="yes">Amount</property>
				<widget class="SwatMoneyEntry" id="amount" />
			</widget>
			<widget class="SwatFormField">
				<property name="title" translatable="yes">Percentage</property>
				<widget class="SwatPercentageEntry" id="percentage" />
			</widget>
			<widget class="SwatFooterFormField">
				<widget class="SwatButton" id="submit_button" />
			</widget>
		</widget>
	</widget>
</swatml>
